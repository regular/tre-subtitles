{
  "name": "tre-texttracks",
  "version": "1.2.0",
  "description": "cpation, subtitle and metadata tracks for audio and video",
  "main": "main.js",
  "browser": "index.js",
  "scripts": {
    "test": "tape test/**"
  },
  "keywords": [
    "tre",
    "subtitles",
    "captions",
    "ssb"
  ],
  "author": "Jan BÃ¶lsche <jan@lagomorph.de> (https://regular.codes/)",
  "license": "AGPL-3.0-or-later",
  "tre-init": {
    "plugins": {
      "tre-client": true,
      "ssb-revisions": true
    },
    "prototypes": {
      ".": true
    },
    "importers": {
      ".": true
    },
    "messages": {
      "texttracks": {
        "root": "%root",
        "branch": "%assets",
        "type": "folder"
      }
    },
    "files": {
      "example.vtt": {
        "path": "example.vtt",
        "content": {
          "branch": "%texttracks",
          "root": "%root"
        }
      }
    }
  },
  "devDependencies": {
    "ssb-revisions": "^1.2.1",
    "tape": "^4.9.2",
    "tre-client": "^2.1.1",
    "tre-editor-shell": "^1.5.0",
    "tre-file-importer": "^3.1.0",
    "tre-finder": "^2.5.0",
    "tre-prototypes": "^1.6.0",
    "tre-split-pane": "^1.0.1"
  },
  "dependencies": {
    "bl": "^2.1.2",
    "brace": "^0.11.1",
    "module-styles": "^1.0.0",
    "mutant": "^3.22.1",
    "pull-stream": "^3.6.9",
    "sodium-native": "^2.2.6",
    "tre-string": "^1.0.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/regular/tre-texttracks.git"
  },
  "bugs": {
    "url": "https://github.com/regular/tre-texttracks/issues"
  },
  "homepage": "https://github.com/regular/tre-texttracks#readme"
}
